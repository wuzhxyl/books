<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="textSize" format="dimension" />
    <attr name="textColor" format="color" />
    <attr name="text" format="string" />
    <attr name="progressIndicatorBackground" format="color" />
    <attr name="progressIndicatorForeground" format="color" />
    <attr name="progressIndicatorShowArrow" format="boolean" />

    <declare-styleable name="ExecutingIndicateButton">
        <attr name="android:clickable" />
        <attr name="android:background" />
        <attr name="textSize" />
        <attr name="textColor" />
        <attr name="text" />
